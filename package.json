{
  "name": "guit",
  "description": "GUI testing tool",
  "keywords": [
    "tool",
    "test",
    "gui",
    "javascript",
    "es6",
    "async",
    "await",
    "json",
    "specs"
  ],
  "main": "lib/guit.js",
  "module": "src/guit.js",
  "version": "2.0.0-dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/shcoder-ru/guit.git"
  },
  "author": "Vasily Shilov <shcoder.ru@ya.ru> (https://github.com/shcoder-ru)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/shcoder-ru/guit/issues"
  },
  "homepage": "https://github.com/shcoder-ru/guit#readme",
  "bin": {
    "guit": "./bin/guit"
  },
  "scripts": {
    "build": "babel src --out-dir dist",
    "test": "mocha --opts tests/test-mocha.opts",
    "coverage": "BABEL_ENV=test mocha --opts tests/coverage-mocha.opts > ./coverage.lcov",
    "make-readme": "jsdoc2md --template README.hbs --files src/guit.js > README.md",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "commander": "^2.11.0",
    "recursive-readdir": "^2.2.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-plugin-transform-coverage": "^0.1.5",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "codecov": "^2.2.0",
    "expect": "^1.20.2",
    "jsdoc-to-markdown": "^3.0.0",
    "mocha": "^3.4.2",
    "mocha-lcov-reporter": "^1.3.0"
  }
}
